{
  "name": "aws-cdk-react-app",
  "version": "1.0.0",
  "bin": {
    "infrastructure": "src/app.ts"
  },
  "scripts": {
    "format": "prettier 'src/**/*.{js,jsx,ts,tsx,css}' --write --config .prettierrc.json",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css}' --config .prettierrc.json",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "build": "tsc",
    "bootstrap": "cdk bootstrap",
    "deploy": "cdk deploy --all",
    "synth": "cdk synth"
  },
  "dependencies": {
    "aws-cdk-lib": "2.135.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.30",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "aws-cdk": "2.135.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.3"
  }
}
